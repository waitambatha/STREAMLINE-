# Database connection settings
# Replace 'your_db_host' with the hostname of your database server, e.g., 'localhost' or 'db.example.com'
DB_HOST=your_db_host

# Replace 'your_db_name' with the name of your PostgreSQL database
DB_NAME=your_db_name

# Replace 'your_db_user' with your PostgreSQL username
DB_USER=your_db_user

# Replace 'your_db_password' with the password for your PostgreSQL user
DB_PASSWORD=your_db_password

# Optional: Add other configuration settings as needed
# Replace 'your_secret_key' with a secret key used for securing your application
SECRET_KEY=your_secret_key

# Optional: Set to 'True' if you want to enable debug mode, otherwise 'False'
DEBUG=True
